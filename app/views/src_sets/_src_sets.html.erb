<div class="thumb-index well">

  <div class="row">
    <% src_sets.each do |ss| %>
        <div class="col-xs-6 col-sm-3 col-md-2">
          <div class="thumbnail">
            <%= render(partial: ss) %>
          </div>
        </div>
    <% end %>
  </div>

  <% if paginate %>
      <%= paginate(src_sets) %>
  <% else %>
      <%= link_to('More sets', :src_sets) %>
  <% end %>

</div>
